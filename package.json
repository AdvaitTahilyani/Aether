  "version": "0.0.0",
  "main": "dist-electron/electron/main.js",
  "scripts": {
    "dev": "vite",
    "rebuild": "electron-rebuild",
    "start:electron": "electron .",
    "build:electron:main": "node electron/electron-build.js",

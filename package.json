    "google-auth-library": "^9.15.1",
    "googleapis": "^146.0.0",
    "node-llama-cpp": "^3.6.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sqlite3": "^5.1.7",
    "zod": "^3.24.2"

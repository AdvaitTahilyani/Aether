{
  "name": "aether",
  "private": true,
  "version": "0.0.0",
  "main": "dist-electron/electron/main.js",
  "scripts": {
    "dev": "vite",
    "start:electron": "electron .",
    "build:electron:main": "node electron/electron-build.js",
    "dev:electron:main": "tsc -p tsconfig.electron.json --watch",
    "dev:electron": "concurrently \"vite build --watch\" \"npm run dev:electron:main\" \"electron .\"",
    "dev:app": "node dev.js",
    "build": "vite build && npm run build:electron:main",
    "preview": "vite preview",
    "build:electron": "vite build && npm run build:electron:main && electron-builder",
    "install:python-deps": "node server/install_dependencies.js",
    "start:flask": "node server/run_server.js"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.0.9",
    "@types/js-base64": "^3.3.1",
    "axios": "^1.8.1",
    "dotenv": "^16.4.7",
    "electron-store": "^8.2.0",
    "googleapis": "^146.0.0",
    "js-base64": "^3.7.7",
    "node-llama-cpp": "^3.6.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sanitize-html": "^2.14.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@tailwindcss/postcss": "^4.0.9",
    "@types/electron-store": "^3.2.2",
    "@types/node": "^22.13.8",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/sanitize-html": "^2.13.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.16",
    "concurrently": "^9.1.0",
